    "Drawing": {
        "ImageExtensions": ".jpg,.jpeg,.png,.bmp",
        "Captcha": {
            "Colors": {
                "ForeHex": "#0066cc",
                "AlternateHex": "#993366",
                "BackgroundHex": "#ccffff",
                "DisturbingHex": "#99ccff"
            },
            "Font": {
                "Source": {
                    "FileName": "font.ttf"
                },
                "Size": 16,
                "Style": "Normal"
            },
            "Noise": {
                "Width": 1,
                "Space": {
                    "X": 10,
                    "Y": 5
                }
            }
        },
        "Scales": [
            {
                "Suffix": "-small",
                "Watermark": "None",
                "MaxSize": {
                    "Width": 100,
                    "Height": 70
                }
            },
            {
                "Suffix": "-large",
                "Watermark": "Text",
                "MaxSize": {
                    "Width": 1000,
                    "Height": 700
                }
            }
        ],
        "Watermark": {
            "IsRandom": true,
            "Text": "librame.net",
            "ImageSource": {
                "FileName": "watermark.png"
            },
            "Colors": {
                "ForeHex": "#ffffff",
                "AlternateHex": "#ccffff",
                "BackgroundHex": "#cccccc",
                "DisturbingHex": "#99ccff"
            },
            "Font": {
                "Source": {
                    "FileName": "font.ttf"
                },
                "Size": 32,
                "Style": "Normal"
            },
            "Location": {
                "X": 20,
                "Y": 10
            }
        }
    },

    "Entity": {
        "DbSources": [
            {
                "EntityProviderOptionsBuilderFactory": "SqliteEntityProviderOptionsBuilderFactory",
                "EntityProviderTypeName": "LibrameStandard.Console.ConsoleEntityProvider, LibrameStandard.Console",
                "Connection": {
                    "DefaultString": "FileName=librame_default.db"
                },
                "Automapping": {
                    "AuditEntity": false,
                    "LowerCaseName": false,
                    // ConsoleEntityProvider 已集成 DbSet<Article>
                    //"EntityTypeNames": [
                    //    "LibrameStandard.Models.Article, LibrameStandard.Models"
                    //],
                    // 使用 ArticleMap 做已集成 Article 的配置器映射
                    "EntityConfiguratorAssemblies": [
                        "LibrameStandard.Models"
                    ]
                }
            }
        ]
    },

    "Network": {
        "Crawler": {
            "UserAgent": "Mozilla/5.0 (Windows NT 6.1; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/62.0.3202.94 Safari/537.36"
        },
        "Message": {
            "ContinueTimeout": 60000,
            "GetewayUrl": "https://sms.contoso.com"
        },
        "Smtp": {
            "Server": "smtp.contoso.com",
            "Port": 587,
            "EmailAddress": "myemail@contoso.com",
            "DisplayName": "MyEmail",
            "Credential": {
                "UserName": "LoginName",
                "Password": "LoginPass"
            }
        }
    },

    "Platform": {
        "Filter": {
            "ConfigSource": {
                "FileName": "librame_filters.json"
            },
            "ReplaceWord": "〔敏感词〕"
        },
        "Schedules": [
            {
                "Job": {
                    "TypeName": "LibrameStandard.Console.ConsoleJob, LibrameStandard.Console",
                    "Identity": {
                        "Name": "job1",
                        "Group": "group1"
                    }
                },
                "Trigger": {
                    "BeginTimeOffset": "now",
                    "EndTimeOffset": "",
                    "CronExpression": "0/2 * * * * ?",
                    "Identity": {
                        "Name": "trigger1",
                        "Group": "group1"
                    }
                }
            }
        ],
        "Storage": {
            "BufferSize": 64
        }
    }
}